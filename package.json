{
  "name": "whois-domain-query",
  "version": "1.0.0",
  "description": "An example project to demonstrate the creation of a CLI using Node.js.",
  "keywords": ["node", "axios", "whois"],
  "repository": "bruceskills/whois-domain-query",
  "funding": "https://github.com/sponsors/bruceskills",
  "author": {
    "name": "Bruno CÃ©sar",
    "email": "bruceskills@gmail.com",
    "url": "https://github.com/bruceskills"
  },
  "main": "index.ts",
  "bin": {
    "whois-cli": "./dist/bin/index.js"
  },
  "type": "module",
  "scripts": {
    "start": "tsx src/bin/index.ts",
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "prebuild": "rimraf dist",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts,.js --max-warnings=0",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write .",
    "format:biome": "biome format --write .",
    "check:biome": "biome check .",
    "lint:biome": "npx @biomejs/biome lint --write ./src"
  },
  "lint-staged": {
    "*.{ts,js,json,md}": ["eslint --fix", "prettier --write", "biome check --fix"]
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "dotenv": "^16.3.1",
    "inquirer": "^12.9.6",
    "minimist": "^1.2.8",
    "ora": "^9.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.19.11",
    "@types/winston": "^2.4.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.35.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1"
  },
  "engines": {
    "node": ">=20"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
